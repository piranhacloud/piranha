{
    "version": "2.0",
    "customHandler": {
        "description": {
            "defaultExecutablePath": "runtime/bin/java",
            "arguments": [
                "--module",
                "helloworld/helloworld.HelloWorldServlet"
            ]
        },
        "enableForwardingHttpRequest": true
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[1.*, 2.0.0)"
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        },
        "logLevel": {
            "default": "Trace"
        }
    }
}
